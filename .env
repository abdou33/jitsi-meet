# Jitsi Docker environment variables (dev mode)

# Network
HTTP_PORT=8080
HTTPS_PORT=8443
TZ=Europe/Paris  # change to your timezone

# No HTTPS, no Let's Encrypt
ENABLE_LETSENCRYPT=0
PUBLIC_URL=http://localhost:8080

# XMPP domain (internal)
XMPP_DOMAIN=meet.local
XMPP_AUTH_DOMAIN=auth.meet.local
XMPP_MUC_DOMAIN=muc.meet.local
XMPP_INTERNAL_MUC_DOMAIN=internal-muc.meet.local
XMPP_GUEST_DOMAIN=guest.meet.local

# Enable all services
ENABLE_AUTH=0
ENABLE_GUESTS=1

# JVB (video bridge)
JVB_PORT=10000
JVB_TCP_HARVESTER_DISABLED=true

# Web UI overrides
JICOFO_COMPONENT_SECRET=secret
JICOFO_AUTH_PASSWORD=secret
JVB_AUTH_PASSWORD=secret
JIGASI_XMPP_PASSWORD=secret
